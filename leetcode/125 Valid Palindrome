  
/**
 * @param {string} s
 * @return {boolean}
 */
var isPalindrome = function(s) {
    let newString = "";
    let regs = /[a-zA-Z\d]/;
    for(let i in s){
        if(regs.test(s[i])) newString += s[i].toUpperCase();      
        
    }
    
    for(let i = 0; i< 1 + newString.length/2;i++){
        if(newString[i]!==newString[newString.length-1-i]){
            return false;
        } 
    }
    console.log(newString);
    return true;
    
    
};
