/**
 * @param {number} n
 */
var SeatManager = function(n) {
    
    this.unreserved = [...Array(n+1).keys()]
    this.reserved = Array(n);
    this.unreserved.shift();
};

/**
 * @return {number}
 */
SeatManager.prototype.reserve = function() {
    let seat = this.unreserved.shift();
    this.reserved[seat-1] = seat;
    //console.log(this.unreserved);
    return seat;
};

/** 
 * @param {number} seatNumber
 * @return {void}
 */
SeatManager.prototype.unreserve = function(seatNumber) {
    
    this.reserved[seatNumber-1] = undefined;
          
    this.unreserved.splice(this.unreserved.findIndex((seat)=>seat>seatNumber), 0, seatNumber);
    
    
};

/** 
 * Your SeatManager object will be instantiated and called as such:
 * var obj = new SeatManager(n)
 * var param_1 = obj.reserve()
 * obj.unreserve(seatNumber)
 */
