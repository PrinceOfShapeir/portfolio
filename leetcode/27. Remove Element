/**
https://leetcode.com/problems/remove-element/submissions/923245299/
 * @param {number[]} nums
 * @param {number} val
 * @return {number}
 */
var removeElement = function(nums, val) {
    
    let i = 0, len = nums.length;
    while(i<len){
        if(nums[i]===val){
            nums.splice(i,1);
        }
        else if(nums[i++]===undefined) return;
    }

};
