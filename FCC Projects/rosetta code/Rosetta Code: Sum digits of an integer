function sumDigits(n) {
  // Good luck!
let array = n.split("");
let sum = 0;
for(let i in array){

  if(/[a-zA-z]/.test(array[i])){
    sum += parseInt(array[i], 16);
    console.log(parseInt(array[i], 16));
    console.log(array[i]);
    //console.log(sum);
  }
  else {
    sum += parseInt(array[i]);
  }

}

return sum;




}
console.log(sumDigits("XYZ"))
