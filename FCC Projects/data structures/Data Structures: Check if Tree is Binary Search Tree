var displayTree = (tree) => console.log(JSON.stringify(tree, null, 2));
function Node(value) {
  this.value = value;
  this.left = null;
  this.right = null;
}
function BinarySearchTree() {
  this.root = null;
}
function isBinarySearchTree(tree) {
  // change code below this line

let isTree = true;
let nodeChecker = (node)=>{
  if(Object.keys(node).length!=3){
    isTree=false;
    return false;
  }
  if(!isTree){
    return false;
  }
  else if(node.value){
    if(node.left){
      if(node.left>node.value){
        isTree = false;
        return isTree;
      }
      else nodeChecker(node.left);
    }
    if(node.right){
      if(node.right<node.value){
        isTree = false;
        return isTree;
      }
      else nodeChecker(node.right);
    }

  }
  else {isTree=false;
    return false;}

}
if(nodeChecker(tree.root)!=false){
  return true;
}


  // change code above this line
}
