var displayTree = tree => console.log(JSON.stringify(tree, null, 2));
function Node(value) {
  this.value = value;
  this.left = null;
  this.right = null;
}
function BinarySearchTree() {
  this.root = null;
  // change code below this line
  this.inorder = ()=>{

    if(this.root === null){
      return this.root;
    }

    let traversals = [];
    let up = "UP";
    let down = "DOWN";
    let left = "LEFT";
    let right = "RIGHT";
    let leftFirst = (node, vertical, lastHorizontal)=>{

      if(vertical==="UP"){
        if(lastHorizontal==="RIGHT"){
          if(node.right){
            return leftFirst(node.right, down, left);
          }
          else return leftFirst()

        }


      }


    }

    }


  




  this.preorder = ()=>{

    if(this.root === null){
      return this.root;
    }
  }
  this.postorder = ()=>{

    if(this.root === null){
      return this.root;
    }
  }

  // change code above this line
}
